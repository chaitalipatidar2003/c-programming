//factorial of a number using user defined function*******************
#include<stdio.h>
int factorial(int n);

void main()
{
    int n,f;
    printf("Enter number \n");
    scanf("%d",&n);
    f=factorial(n);

    printf("factorial=%d",f);


}

int factorial(int n)
{
    int i,fact=1;
    for ( i = n; i >=1; i--)
    
       fact=fact*i;
       return(fact);
    
    
}
