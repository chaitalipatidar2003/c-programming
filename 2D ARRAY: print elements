//*************2D ARRAY ****************************

#include <stdio.h>

void main()
{
    int a[3][3], i, j;

    printf("Enter the elements of 2D array- ");
    for (i = 0; i <= 2; i++)
        for (j = 0; j <= 2; j++)
            scanf("%d ", &a[i][j]);

    printf("Elements of 2D array - \n");
    for (i = 0; i <= 2; i++)
    {
        for (j = 0; j <= 2; j++)
            printf("%d ", a[i][j]);
        printf("\n");
    }
}
