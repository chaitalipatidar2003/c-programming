//**********************************************STRUCTURE:2 OBJECTS****************************************
#include <stdio.h>

struct emp
{
    int code;
    char name[20];
    char dept[20];
    float salary;
};

void main()
{
    struct emp e1,e2;
    printf("Enter code,name,department,salary");
    scanf("%d %s %s %f", &e1.code, &e1.name, &e1.dept, &e1.salary);

    printf("Enter code,name,department,salary");
    scanf("%d %s %s %f", &e2.code, &e2.name, &e2.dept, &e2.salary);

    printf("CODE: %d\n", e1.code);
    printf("NAME: %s\n", e1.name);
    printf("DEPARTMENT: %s\n", e1.dept);
    printf("SALARY: %f\n", e1.salary);

    printf("CODE: %d\n", e2.code);
    printf("NAME: %s\n", e2.name);
    printf("DEPARTMENT: %s\n", e2.dept);
    printf("SALARY: %f\n", e2.salary);
}
