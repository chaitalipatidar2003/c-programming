//***************************FILE HANDLING: read the content************************************************

#include<stdio.h>

void main()
{
    FILE *fp;
    char ch;
    
    fp=fopen("University.txt","r");

    while (1)
    {
        ch=fgetc(fp);
        if(ch==EDF)
        break;
        printf("%c",ch);
    }
    
}
