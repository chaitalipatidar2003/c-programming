#include <stdio.h>

void main()
{
    float a, b, res;
    char op;

    printf("Enter a and b");
    scanf("%f %f", &a, &b);

    printf("Enter operator");
    scanf("%c", &op);

    switch (op)
    {
    case '+':
        res = a + b;
        printf("Addition= %f",res);
        break;
    case '-':
        res = a - b;
        printf("Subraction=%f",res);
        break;
    case '*':
        res = a * b;
        printf("Multiplicaion=%f",res);
        break;
    case '/':
        res = a / b;
        printf("Division=%f",res);
        break;
    default:
        printf("invalid operator\n");
        
    }
}
