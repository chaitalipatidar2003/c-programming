// Recursion:table
#include <stdio.h>
void show(int a);

void main()
{
    int n;
    printf("Enter n");
    scanf("%d", &n);

    show(1);
}

void show(int a)
{
    int n;

    if (a <= 10)
    printf("%d * %d = %d \n", n, a,n*a);
   show(a + 1);
}
