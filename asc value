//asc value****************using character********************
#include<stdio.h>

void main()
{
    int i,j;

    for ( i = 65; i <=90; i++)
    {
        for ( j= 1; j <=5; j++)
        {
            printf(" %c ",i);
        }
        printf("\n");
    }
    
}


