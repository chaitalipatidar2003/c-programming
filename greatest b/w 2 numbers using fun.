//greatest b/w 2 numbers using fun.**********
#include<stdio.h>

int biggest(int a, int b);

void main()
{
    int a,b;
    printf("Enter a and b");
    scanf("%d %d",&a,&b);
    biggest(a,b);
    printf("The biggest value is %d",biggest(a,b));
}

int biggest(int a,int b)
{
    if(a>b)
    return(a);
    else
    return(b);

}
