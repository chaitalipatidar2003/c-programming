// Function :FIBBONACCI SERIES ***************************

#include<stdio.h>
void fibo(int);

void main()
{
    int n;
    printf("Enter the limit");
    scanf("%d",&n);

    fibo(n);
}

void fibo(int n)
{
    int i, a=-1,b=1,c;
    for( i= 1; i <=n; i++)
    {
        c=a+b;
        printf("%d\n",c);
        a=b;
        b=c;

    }
    
    
}
