{
    int m1, m2, m3, total;
    float per;

    printf("Enter m1, m2, m3");
    scanf("%d%d%d", &m1, &m2, &m3);

    total = m1 + m2 + m3;
    per = (total * 100) / 300;
    printf("Total marks are %d and  percentage are %f  ",total,per);

    if (per >= 60)
        printf("I grade");
    else if(per>=50 && per<60)

    printf("II grade");

    else if(per>=40 && per<50)
    printf("III grade");

    else 
    printf("Fail");

}
