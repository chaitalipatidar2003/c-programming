//****************ARRAY OF POINTER*********************************

#include<stdio.h>

void main()
{
    int a=10,b=20,c=30;
    int *p[3];

    p[0]=&a;
    p[1]=&b;
    p[2]=&c;

    for (int  i = 0; i <=2; i++)
    {
        printf("%d \n",*p[i]);
    }
    
    
}
