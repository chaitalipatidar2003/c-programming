// factorial using goto

#include <stdio.h>

void main()
{
    int i, n, fact = 1;
    printf("Enter a number\n");
    scanf("%d", &n);

ft:  fact = fact * i;
    i++;
    if (i <= n)
        goto ft;

    printf("The factorial of n is %d  ",fact);
}
