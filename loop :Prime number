// PRIME NUMBER OR NOT*************
#include <stdio.h>

void main()
{
    int n, div;
    // flag=0;
    div=2;
    printf("Enter the value of n");
    scanf("%d", &n);

    while( div < n)
    {

        if (n % div == 0)
        {
            printf("Number is not prime\n ");
          exit(0);
        }
        else
        div = div +1;
    }
    printf("Number is prime");

}
