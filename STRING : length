//STRING : length*******************************
#include <stdio.h>
#include <string.h>

void main()
{
    char password[20];
    int len;

    printf("Enter password ");
    scanf("%s", password);

    len = strlen(password);

    printf("Length of password is %ld \n ", strlen(password));

    if (len < 8)
        printf("Password is too short\n ");
    else
        printf("Password is ok\n");

            
}
