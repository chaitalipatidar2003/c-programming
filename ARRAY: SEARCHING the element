//***************SEARCHING the element of an array***********

#include<stdio.h>

void main()
{
    int a[5],i,key,found=0;

    printf("Enter the elements of an array");
    for(i=0;i<=4;i++)
    scanf("%d",&a[i]);

    printf("Element of an array to be search is ");
    scanf("%d",&key);

    for ( i = 0; i <= 4; i++)
    {
        if(a[i]==key)
        {
            found=1;
            break;
        }
    }
    {
        if (found=1)
        {
            printf("Element found ");
        }
        else
        printf("Element not found");
    }
    

}
