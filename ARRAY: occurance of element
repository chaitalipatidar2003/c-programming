//find the occurance of an element of 1 array**************
//ARRAY: occurance of element

#include<stdio.h>

void main()
{
    int a[5],i,key,count=0;

    printf("Enter the elements of array");
    for(i=0;i<=4;i++)
    scanf("%d",&a[i]);

    printf("Enter the element to be search and count");
    scanf("%d",&key);

    for ( i = 0; i <=4; i++)
    {
        if(a[i]==key)
        count++;
    }

    if (count!=0)

        printf("\n Element found %d times ",count);
    
    else 
    printf("\n Elements not found ");
    
}
